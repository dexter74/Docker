services: #
###########

##################################################################################

# external_links ou links : Selon si le conteneur MariaDB est dans le stack ou pas
# Base de donnée des images : https://fleet.linuxserver.io/
# Certains conteneur mettent du temps à démarrer. (Voir log)

##################################################################################
  MariaDB:
    image: linuxserver/mariadb
    container_name: CN_MariaDB
    restart: unless-stopped
    environment:
      - PUID=1001
      - PGID=74240
      - TZ=Europe/Paris
      - MYSQL_ROOT_PASSWORD=root
      - MYSQL_DATABASE=database
      - MYSQL_USER=admin
      - MYSQL_PASSWORD=admin
    volumes:
      - MariaDB:/config
    labels:
       cacher: 'non'
##################################################################################

##################################################################################
# Streamer Musique
  Airsonic:
    container_name: CN_Airsonic
    image: linuxserver/airsonic
    restart: 'no'
    environment:
      - PUID=1001
      - PGID=74240
      - TZ=Europe/Paris
    ports:
      - 1000:4040
    volumes:
      - Airsonic:/config
      - Airsonic:/media
      - Airsonic:/playlists
      - Airsonic:/podcasts
      - Musique:/music
    labels:
       cacher: 'non'
##################################################################################

##################################################################################
