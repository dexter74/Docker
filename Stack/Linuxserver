# Streamer Musique
  Airsonic:
    container_name: 'CN_Airsonic'
    image: 'linuxserver/airsonic'
    network_mode: 'bridge'
    restart: 'no'
    environment:
      PUID: '1001'
      PGID: '74240'
      TZ: 'Europe/Paris'
    ports:
      - '1000:4040'
    volumes:
      - 'Airsonic:/config'
      - 'Airsonic:/media'
      - 'Airsonic:/playlists'
      - 'Airsonic:/podcasts'
      - 'Musique:/music'
    labels:
       cacher: 'non'
