    labels:
       cacher: 'non'
##################################################################################

##################################################################################
# Hebergement Image (Requis : MariaDB)
  Chevereto:
    container_name: CN_Chevereto
    image: linuxserver/chevereto
    restart: 'no'
    environment:
      - PUID=1001
      - PGID=74240
      - TZ=Europe/Paris
    ports:
      - 1002:80
    volumes:
      - Chevereto:/config
      - Chevereto:/data
    links:
      - MariaDB:mysql
    depends_on:
     - MariaDB
    labels:
       cacher: 'non'

